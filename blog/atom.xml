---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>My Blog</title>
	<link href="http://savannahm4215.github.io/blog/atom.xml" rel="self"/>
	<link href="http://savannahm4215.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://savannahm4215.github.io/blog</id>
	<author>
		<name>Savannah Major</name>
		<email>savannahm4215@afsenyc.org</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://savannahm4215.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://savannahm4215.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>

 <style>
        h1 {
            color: rgb(0, 0, 0);
        }
        body {
            background-color: rgb(172, 0, 245);
        }
        </style>

